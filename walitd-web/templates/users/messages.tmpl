<h2> Sent messages</h2>
<table class="table-bordered">
    <tr><td>Reciever</td><td>Message</td><td>Created</td></tr>
    {{range .Sent -}} <tr><td>{{.Reciever}}</td><td>{{.Text}}</td><td>{{.Created}}</td></tr>{{- end}}
</table>
<br>
<h2>Recieved messages</h2>
<table class="table-bordered">
    <tr><td>Sender</td><td>Message</td><td>Created</td></tr>
    {{range .Recieved -}} <tr><td>{{.Sender}}</td><td>{{.Text}}</td><td>{{.Created}}</td></tr>{{- end}}
</table>
<br>
